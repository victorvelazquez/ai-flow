{
  "name": "ai-flow-dev",
  "version": "2.4.3",
  "description": "AI-powered development workflow from idea to production. Generate specs, plan features, and build with AI assistance throughout your project lifecycle.",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "ai-flow": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "start": "node dist/cli.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"__tests__/**/*.js\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "flow",
    "workflow",
    "development",
    "automation",
    "spec-driven-development",
    "documentation",
    "claude",
    "copilot",
    "cursor",
    "gemini",
    "cli",
    "feature-development"
  ],
  "author": "Victor Velazquez <victorvelazqueza@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/victorvelazquez/ai-flow.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.1.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "prompts",
    "templates",
    "README.md",
    "LICENSE"
  ]
}